      "target": "plugin::users-permissions.user",
      "inversedBy": "invoices"
    },
    "trip": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::trip.trip",
      "inversedBy": "invoices"
    },
    "seats": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::seat.seat",
