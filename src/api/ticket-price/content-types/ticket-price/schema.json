{
  "kind": "collectionType",
  "collectionName": "ticket_prices",
  "info": {
    "singularName": "ticket-price",
    "pluralName": "ticket-prices",
    "displayName": "TicketPrice"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "price": {
      "type": "decimal"
    },
    "startDate": {
      "type": "datetime"
    },
    "endDate": {
      "type": "datetime"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "ACTIVE",
        "INACTIVE",
        "DEPRECATED"
      ],
      "required": true
    },
    "promotion": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::promotion.promotion",
      "inversedBy": "ticket_prices"
    },
    "trips": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::trip.trip",
      "mappedBy": "ticket_price"
    }
  }
}
