{
  "kind": "collectionType",
  "collectionName": "promotions",
  "info": {
    "singularName": "promotion",
    "pluralName": "promotions",
    "displayName": "Promotion",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "promotionName": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "discountType": {
      "type": "enumeration",
      "enum": [
        "Tặng tiền",
        "Chiết khấu hóa đơn"
      ],
      "required": true
    },
    "discountValue": {
      "type": "decimal"
    },
    "startDate": {
      "type": "datetime"
    },
    "endDate": {
      "type": "datetime"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Hoạt động",
        "Không hoạt động"
      ],
      "required": true,
      "default": "Hoạt động"
    },
    "IDPromotion": {
      "type": "string",
      "default": "MK"
    },
    "detail_promotions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::detail-promotion.detail-promotion",
      "mappedBy": "promotion"
    }
  }
}
